// to print two eleament in array which addition equal to 10

public class TwoAdd
{
public static void main (String args[])
{
int a[]={4, 8, 6, 7, 0, 7, 1, 7, 8};

for(int i=0;i<a.length;i=i+1)
{
for(int j=i+1;j<a.length;j=j+1)
{
if(a[i]+a[j]==10)
{

System.out.println(a[i] + "+" + a[j] + "= 10" );
}
}
}
}
}
